{
    "name": "<%= projectName %>",
    "version": "1.0.0",
    "description": "A simple @kissflow/c3 app that targets Kissflow's <%= projectTarget %>.",
    "main": "index.js",
    "scripts": {
        "dev": "c3-scripts dev",
        "build": "c3-scripts build",
        "zip": "zip -r c3-app.zip ./dist",
        "unzip": "rm -rf ./zip && unzip c3-app.zip -d ./c3-app"
    },
    "type": "module",
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@shibi-snowball/c3-scripts": "<%= latestC3ScriptsVersion %>",
        "@shibi-snowball/c3-model": "<%= latestC3ModelVersion %>",
        "unzipper": "^0.10.14",
        "zip": "^1.2.0"
    },
    "engines": {
        "node": ">=16.0.0 <17.0.0"
    },
    "eslintConfig": {
        "extends": ["react-app"],
        "rules": {
            "no-restricted-globals": [
                "error",
                {
                    "name": "window",
                    "message": "Don't use browser's `window` object."
                },
                {
                    "name": "document",
                    "message": "Don't use browser's `document` object."
                }
            ]
        }
    },
    "keywords": [
        "kissflow-<%= projectTarget %>",
        "lcnc",
        "no-code",
        "low-code",
        "platform",
        "c3",
        "c3-app",
        "custom-component-creator",
        "react",
        "cli",
        "kissflow",
        "custom-components",
        "custom-fields"
    ],
    "dependencies": {
        "react": "<%= reactVersion %>",
        "react-dom": "<%= reactDomVersion %>"
    }
}
